@desc	2. Form 변수 선언 ---------------
********************************************************************************************/
this.paramStr;
this.paramNum;
this.paramObj;
var sSplitChar = this.gfn_getApplication().MultiTxtSplitChar;
var CHK_TRUE 	= "Y";
var CHK_FALSE 	= "N";

//공통코드 세팅 -- 할 일이 있다면 ...
// this.COMBO_SET_STR = "ds_cmbPaymentTerms"+"#"+"ZTERM"		+" "
