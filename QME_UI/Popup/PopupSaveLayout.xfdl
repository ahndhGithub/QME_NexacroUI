@desc	7. Common Function
********************************************************************************************/
/*
 *	조회
 */
this.cfn_search = function()
{
	this.tfn_FormatSearch();
};

/*
 *	입력
 */
this.cfn_add = function()
{
};

/*
 *	삭제
 */
this.cfn_del = function()
{
	{	//그리드한줄 삭제버튼 클릭후 바로 저장
		this.DivBtnAddDel_GridPoList00.form.btn_del.click();
		this.tfn_FormatSave();
	}
